{
  "name": "mysa-js-sdk",
  "version": "0.0.0",
  "license": "MIT",
  "private": false,
  "description": "A JavaScript SDK for accessing Mysa smart thermostats.",
  "keywords": [
    "mysa",
    "thermostat",
    "sdk",
    "api"
  ],
  "publishConfig": {
    "provenance": true
  },
  "author": {
    "name": "Pascal Bourque",
    "email": "pascal@cosmos.moi"
  },
  "bugs": {
    "url": "https://github.com/bourquep/mysa-js-sdk/issues"
  },
  "homepage": "https://github.com/bourquep/mysa-js-sdk",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bourquep/mysa-js-sdk.git"
  },
  "files": [
    "README.md",
    "LICENSE.txt",
    "dist"
  ],
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=22.4.0"
  },
  "browser": false,
  "scripts": {
